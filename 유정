과제1. 네임스페이스 활용 코드 만들기
  
#include <iostream>

namespace Calculator {
    int x(int a, int b) {
        return a + b;
    }

    int y(int a, int b) {
        return a * b;
    }
}

int main() {
    std::cout << "3 + 5 = " << Calculator::x(3, 5) << std::endl;
    std::cout << "3 * 5 = " << Calculator::y(3, 5) << std::endl;
    return 0;
}


  
